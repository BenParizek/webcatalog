{
  "name": "WebCatalog",
  "version": "7.7.0",
  "description": "WebCatalog helps you run thousands of web apps like real apps.",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node hacky.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./dist.js ./hacky.js ./public ./src --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cross-env API_URL=http://localhost:3000/api electron .\"",
    "dist-dev": "yarn build && node dist.js",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/webcatalog",
  "author": {
    "name": "Quang Lam",
    "email": "support@webcatalog.io",
    "url": "https://webcatalog.io"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "@webcatalog/molecule": "1.6.0",
    "electron-is-dev": "0.3.0",
    "electron-updater": "2.16.1",
    "electron-window-state": "4.1.1",
    "follow-redirects": "1.2.5",
    "fs-extra": "4.0.2",
    "request-promise": "4.2.2",
    "tmp": "0.0.33",
    "yargs-parser": "8.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chalk": "2.3.0",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "cross-env": "5.1.1",
    "electron": "1.7.9",
    "electron-builder": "19.45.4",
    "electron-builder-squirrel-windows": "19.45.0",
    "electron-rebuild": "1.6.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "material-ui": "1.0.0-beta.21",
    "material-ui-icons": "1.0.0-beta.17",
    "mocha": "4.0.1",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "replace-in-file": "3.0.0",
    "request": "2.83.0",
    "semver": "5.4.1",
    "spectron": "3.7.2",
    "typeface-roboto": "0.0.45",
    "wait-on": "2.0.2"
  },
  "homepage": "./",
  "proxy": "https://webcatalog.io"
}
