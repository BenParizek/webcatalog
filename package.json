{
  "name": "WebCatalog",
  "version": "7.0.5",
  "description": "WebCatalog is an app store with thousands of exclusive apps for your Mac and PC.",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node hacky.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cross-env API_URL=http://localhost:3000/api electron .\"",
    "dist-dev": "yarn build && node dist.js",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/webcatalog",
  "author": {
    "name": "WebCatalog Team",
    "email": "support@webcatalog.io",
    "url": "https://webcatalog.io"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "@webcatalog/molecule": "1.0.7",
    "electron-is-dev": "0.3.0",
    "electron-updater": "2.10.1",
    "electron-widevinecdm": "3.1.2",
    "electron-window-state": "4.1.1",
    "follow-redirects": "1.2.4",
    "fs-extra": "4.0.2",
    "request-promise": "4.2.2",
    "tmp": "0.0.33",
    "yargs-parser": "7.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chalk": "2.1.0",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "cross-env": "5.0.5",
    "electron": "1.8.1",
    "electron-builder": "19.34.1",
    "electron-builder-squirrel-windows": "19.34.0",
    "electron-rebuild": "1.6.0",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "material-ui": "1.0.0-beta.13",
    "material-ui-icons": "1.0.0-beta.14",
    "mocha": "3.5.3",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.14",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "replace-in-file": "2.5.4",
    "request": "2.83.0",
    "spectron": "3.7.2",
    "typeface-roboto": "0.0.35",
    "wait-on": "2.0.2"
  },
  "optionalDependencies": {
    "windows-shortcuts": "0.1.6"
  },
  "homepage": "./",
  "proxy": "https://webcatalog.io"
}
